    <TextView
        android:id="@+id/tv_case_manage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="案例管理" />

    <TextView
        android:id="@+id/tv_team_manage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="团队管理" />
</LinearLayout>